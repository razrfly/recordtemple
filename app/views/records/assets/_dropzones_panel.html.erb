<div class="row">
  <div class="col-md-6">
    <div class="panel panel-default panel-profile">
      <div class="panel-body">
        <h5 class="m-t-0">Add Photos</h5>

        <div id="photos">
          <%= content_tag :div,
            data: { url: record_photos_path(f.object) },
            class: "dropzone m-t", id: 'photos' do %>
          <% end %>
        </div>
      </div>
    </div>
  </div>

  <div class="col-md-6">
    <div class="panel panel-default panel-profile">
      <div class="panel-body">
        <h5 class="m-t-0">Songs</h5>

        <div id="songs">
           <%= content_tag :div,
            data: { url: record_songs_path(f.object) },
            class: "dropzone m-t", id: 'songs' do %>
           <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
